{
  "name": "maoureu",
  "version": "1.0.0",
  "author": "Gurimarukin",
  "license": "MIT",
  "scripts": {
    "clean": "rm -fr dist/*",
    "prebuild": "yarn run clean",
    "build": "tsc",
    "maoureuFetch": "NODE_OPTIONS='--unhandled-rejections=strict' ts-node src/maoureuFetch.ts",
    "maoureuHtml": "NODE_OPTIONS='--unhandled-rejections=strict' ts-node src/maoureuHtml.ts",
    "maoureuStart": "parcel output/maoureu/index.html --dist-dir build",
    "maoureuBuild": "parcel build output/maoureu/index.html --dist-dir build",
    "maoureu": "yarn run maoureuFetch && yarn run maoureuHtml && yarn run maoureuBuild",
    "prettier": "prettier --check src test",
    "prettierFix": "prettier --write src test",
    "lint": "eslint --cache --max-warnings 0 src test",
    "lintFix": "eslint --fix --cache --max-warnings 0 src test",
    "jestUnit": "jest --testPathPattern=.*\\.spec\\.ts",
    "jestCoverage": "jest --ci --coverage",
    "style": "yarn run prettier && yarn run lint",
    "test": "yarn run style && yarn run jestUnit"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@type-config/strict": "^1.1.1",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.8",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-fp-ts": "^0.3.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "parcel": "^2.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "fp-ts": "^2.11.6",
    "got": "^11.8.3",
    "io-ts": "^2.2.16",
    "jsdom": "^19.0.0",
    "monocle-ts": "^2.3.11",
    "newtype-ts": "^0.3.4",
    "rimraf": "^3.0.2"
  }
}
